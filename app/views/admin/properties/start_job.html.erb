<%= semantic_form_for :data, url: { action: :start_worker }, method: :post do |f| %>
    <%= f.inputs "Form" do %>
        <%= f.input :url %>
        <%= f.input :total_pages %>
    <% end %>
    <%= f.actions do %>
        <%= f.action :submit, as: :button, label: "Start Scraping" %>
    <% end %>
<% end %>